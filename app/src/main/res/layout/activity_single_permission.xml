<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <Button
        android:id="@+id/btn_camera"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="CAMERA" />


    <Button
        android:id="@+id/btn_audio"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="RECORD_AUDIO" />

    <Button
        android:id="@+id/btn_accounts"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="GET_ACCOUNTS" />

    <Button
        android:id="@+id/btn_call_phone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="CALL_PHONE" />

    <Button
        android:id="@+id/btn_read_phone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="PERMISSION_READ_PHONE_STATE" />

    <Button
        android:id="@+id/btn_file_location"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="ACCESS_FINE_LOCATION" />

    <Button
        android:id="@+id/btn_coarse_location"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="ACCESS_COARSE_LOCATION" />

    <Button
        android:id="@+id/btn_read_storage"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="READ_EXTERNAL_STORAGE" />

    <Button
        android:id="@+id/btn_write_storage"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="WRITE_EXTERNAL_STORAGE" />

</LinearLayout>
